		</copy>
		<copy todir="builds/filebasedobjectidm">
			<fileset dir="../../FileBasedObjectIDM">
				<include name="*.xml" />
			</fileset>
		</copy>
		<jar destfile="builds/pluginlibs/bimserver-${version}-filebasedobjectidm.jar">
			<fileset dir="builds/filebasedobjectidm"/>
		</jar>
	</target>
